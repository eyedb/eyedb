	    <term>
	      <option>--dbfile=<replaceable>dbfile</replaceable></option>
	    </term>
	    <listitem>
	      <para>Specifies the data base file.</para>
	    </listitem>
	  </varlistentry>

	  <varlistentry>
	    <term>
	      <option>--filedir=<replaceable>filedir</replaceable></option>
	    </term>
	    <listitem>
	      <para>Specifies the file directory.</para>
	    </listitem>
	  </varlistentry>

	  <varlistentry>
	    <term>
	      <option>--datafiles=<replaceable>datafile</replaceable><arg choice="opt">:<replaceable>name</replaceable></arg><arg choice="opt">:<replaceable>sizeMb</replaceable></arg><arg choice="opt">:sizeslot</arg><group choice="opt"><arg choice="plain">phy</arg><arg choice="plain">log</arg></group></option>
	    </term>
	    <listitem>
	      <para>Specifies data files for the database.</para>
